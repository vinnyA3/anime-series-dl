{
  "name": "animego-dl",
  "version": "3.0.0",
  "description": "A cli tool to download your favorite anime series from gogoanimes",
  "scripts": {
    "test": "mocha --recursive './lib/**/test/*.test.js'",
    "test:watch": "npm run test -- --growl false -w --watch-files ./lib/**/test/*.test.js ./lib/**/*.js",
    "build": "npm run tsc && npm run test",
    "clean": "rm -rf dist",
    "refresh": "rm -rf node_modules && rm -rf dist && rm package-lock.json && npm install",
    "lint": "eslint . --ext .ts",
    "tsc": "tsc --build"
  },
  "bin": {
    "animego-dl": "./dist/bin/index.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinnyA3/anime-series-dl.git"
  },
  "homepage": "https://github.com/vinnyA3/anime-series-dl#readme",
  "keywords": [
    "anime",
    "scraper",
    "anime download",
    "node",
    "otaku"
  ],
  "author": "vinnyA3",
  "bugs": {
    "url": "https://github.com/vinnyA3/anime-series-dl/issues"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "commander": "^9.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "mocha": "^9.1.4",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "mocha": {
    "diff": true,
    "extension": [
      "js"
    ],
    "reporter": "spec",
    "timeout": "2000",
    "ui": "bdd",
    "growl": true,
    "inline-diffs": false
  }
}
