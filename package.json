{
  "name": "animego-dl",
  "version": "2.0.0",
  "description": "A cli tool to download your favorite anime series from gogoanimes",
  "main": "bin/index.js",
  "scripts": {
    "test": "mocha --recursive './lib/**/test/*.test.js'",
    "test:watch": "npm run test -- --growl false -w --watch-files ./lib/**/test/*.test.js ./lib/**/*.js"
  },
  "bin": {
    "animego-dl": "./bin/index.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinnyA3/anime-series-dl.git"
  },
  "homepage": "https://github.com/vinnyA3/anime-series-dl#readme",
  "keywords": [
    "anime",
    "scraper",
    "anime download",
    "node",
    "otaku"
  ],
  "author": "vinnyA3",
  "bugs": {
    "url": "https://github.com/vinnyA3/anime-series-dl/issues"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "commander": "^9.0.0"
  },
  "devDependencies": {
    "mocha": "^9.1.4"
  },
  "mocha": {
    "diff": true,
    "extension": [
      "js"
    ],
    "reporter": "spec",
    "timeout": "2000",
    "ui": "bdd",
    "growl": true,
    "inline-diffs": false
  }
}
